{
  "name": "vega-media-info",
  "version": "2.8.0",
  "description": "An image and video meta data extracter/updater",
  "browser": {
    "child_process": false,
    "fs": false
  },
  "main": "index.js",
  "scripts": {
    "pretest": "cp test_data/img1.jpg test_data/fresh.jpg",
    "test": "mocha",
    "lint": "eslint --ext .js -f ./node_modules/eslint-friendly-formatter . test",
    "lint:fix": "eslint --ext .js -f ./node_modules/eslint-friendly-formatter --fix . test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/softbrix/vega_media_info/"
  },
  "keywords": [
    "exif",
    "media",
    "video",
    "image",
    "jpg",
    "jpeg",
    "mpeg",
    "mpg",
    "meta"
  ],
  "author": "Andreas Sehr",
  "license": "GPL-3.0",
  "dependencies": {
    "hemmesta-jpeg-meta": "1.3.2",
    "kopparmora-xmp-reader": "1.1.4",
    "mp4box": "^0.4.1",
    "node-iptc": "^1.0.5",
    "vimmerby-exif": "0.10.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "7.28.0",
    "eslint-config-standard": "12.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "mocha": "^8.2.1"
  }
}
