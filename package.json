{
  "name": "vega_media_info",
  "version": "2.0.2",
  "description": "An image and video meta data extracter/updater",
  "main": "index.js",
  "scripts": {
    "pretest": "cp test_data/img1.jpg test_data/fresh.jpg",
    "test": "mocha",
    "lint": "eslint --ext .js -f ./node_modules/eslint-friendly-formatter . test",
    "lint:fix": "eslint --ext .js -f ./node_modules/eslint-friendly-formatter --fix . test"
  },
  "repository": {
    "type": "git",
