{
  "name": "vega_media_info",
  "version": "2.4.1",
  "description": "An image and video meta data extracter/updater",
  "main": "index.js",
  "scripts": {
    "pretest": "cp test_data/img1.jpg test_data/fresh.jpg",
    "test": "mocha",
    "lint": "eslint --ext .js -f ./node_modules/eslint-friendly-formatter . test",
    "lint:fix": "eslint --ext .js -f ./node_modules/eslint-friendly-formatter --fix . test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/softbrix/vega_media_info/"
  },
  "keywords": [
    "exif",
    "media",
    "video",
    "image",
    "jpg",
    "jpeg",
    "mpeg",
    "mpg",
    "meta"
  ],
  "author": "Andreas Sehr",
  "license": "GPL-3.0",
  "dependencies": {
    "exif": "^0.6.0",
    "hemmesta-jpeg-meta": "1.3.1",
    "kopparmora-xmp-reader": "1.1.2",
    "mp4box": "0.3.19",
    "node-iptc": "^1.0.5"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "eslint": "5.16.0",
    "eslint-config-standard": "10.2.1",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-plugin-html": "3.2.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "mocha": "4.1.0"
  }
}
